<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <groups>
        <run>
            <include name="Smoke"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="Utils.Listeners"></listener>
    </listeners>
    <test verbose="2" preserve-order="true" name="From Fill and Error Validation">
        <classes>
            <class name="Andriod.ECOM_TC_1"></class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="End to End Flow">
        <classes>
            <class name="Andriod.ECOM_TC_4"></class>
        </classes>
    </test>
</suite>